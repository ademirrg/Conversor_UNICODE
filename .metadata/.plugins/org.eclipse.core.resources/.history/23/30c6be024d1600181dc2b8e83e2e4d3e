package main;

public class Conversor {
	
	public static void main(String[] args) {
//		char ch = 'a';
//		System.out.println((int)ch);
		char c;
		String[] campo;
		String str = "ABC";
		campo = str.split("");
		
		for (int i=0;i<str.length();i++) {
			c = campo[i].charAt(i);
			System.out.println(c);
			System.out.println((int)c);
		}
	}
}
