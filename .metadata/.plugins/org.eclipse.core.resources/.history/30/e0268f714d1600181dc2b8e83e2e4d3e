package main;

public class Conversor {
	
	public static void main(String[] args) {
		char c;
		String[] campo;
		String str = "ABC DEF";
		campo = str.split("");
		
		
		
		for (int i=0;i<str.length();i++) {
			c = campo[i].charAt(0);
			System.out.println(c);
			System.out.println((int)c);
		}
	}
}
